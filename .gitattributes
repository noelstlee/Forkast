# Track all parquet files with Git LFS
*.parquet filter=lfs diff=lfs merge=lfs -text

# Specifically track processed data files
data/processed/**/*.parquet filter=lfs diff=lfs merge=lfs -text
data/processed/ga/**/*.parquet filter=lfs diff=lfs merge=lfs -text
# Track large JSON files if needed
data/raw/*.json.gz filter=lfs diff=lfs merge=lfs -text
